{
	"name": "portfolio",
	"version": "0.1.2",
	"type": "module",
	"author": "Gustavo Matheus <gustavo.morinaga27@gmail.com>",
	"description": "Make a description",
	"scripts": {
		"dev": "pnpm run partytown && vite dev --open",
		"build": "pnpm run partytown && vite build",
		"preview": "pnpm run partytown && vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"partytown": "partytown copylib static/~partytown",
		"release": "standard-version",
		"release-as-major": "pnpm run release --release-as major",
		"release-as-minor": "pnpm run release --release-as minor",
		"release-as-patch": "pnpm run release --release-as patch",
		"push-release": "git push --follow-tags origin main",
		"prepare": "husky install"
	},
	"devDependencies": {
		"@builder.io/partytown": "0.7.5",
		"@fontsource/caveat": "4.5.10",
		"@fontsource/courier-prime": "4.5.9",
		"@fontsource/karla": "4.5.11",
		"@fontsource/orbitron": "4.5.11",
		"@fontsource/righteous": "4.5.7",
		"@iconify/svelte": "3.0.1",
		"@iconify/types": "2.0.0",
		"@macfja/svelte-persistent-store": "2.2.1",
		"@playwright/test": "1.29.2",
		"@sveltejs/adapter-vercel": "^1.0.5",
		"@sveltejs/kit": "1.2.6",
		"@tailwindcss/typography": "0.5.9",
		"@types/html-minifier": "4.0.2",
		"@types/three": "0.148.1",
		"@typescript-eslint/eslint-plugin": "5.49.0",
		"@typescript-eslint/parser": "5.49.0",
		"@vercel/analytics": "^0.1.8",
		"atropos": "1.0.2",
		"autoprefixer": "10.4.13",
		"cssnano": "5.1.14",
		"daisyui": "2.47.0",
		"detect-gpu": "5.0.8",
		"eslint": "8.32.0",
		"eslint-config-prettier": "8.6.0",
		"eslint-plugin-svelte3": "4.0.0",
		"html-minifier": "4.0.0",
		"husky": "8.0.3",
		"lint-staged": "13.1.0",
		"postcss": "8.4.21",
		"postcss-load-config": "4.0.1",
		"prettier": "2.8.3",
		"prettier-plugin-svelte": "2.9.0",
		"sass": "1.57.1",
		"standard-version": "9.5.0",
		"svelte": "3.55.1",
		"svelte-check": "3.0.3",
		"svelte-preprocess": "5.0.1",
		"swiper": "8.4.6",
		"tailwind-scrollbar": "2.1.0",
		"tailwindcss": "3.2.4",
		"three": "0.148.0",
		"tslib": "2.4.1",
		"typescript": "4.9.4",
		"vite": "4.0.4",
		"vitest": "0.28.1",
		"web-vitals": "^3.1.1"
	}
}
