		"swiper": "9.2.4",
		"tailwind-scrollbar": "3.0.0",
		"tailwindcss": "3.3.2",
		"three": "0.152.2",
		"tslib": "2.5.0",
		"typescript": "5.0.4",
		"unified": "10.1.2",
